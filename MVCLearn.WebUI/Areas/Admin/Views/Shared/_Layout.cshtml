@using StackExchange.Profiling
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @RenderSection("Styles", false)
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.css" rel="stylesheet" />
    <link href="~/assets/css/Admin/Common.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.1.1.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/node_modules/vue/dist/vue.js"></script>
    <script src="~/Scripts/js-cookie/js.cookie.js"></script>
    <script src="~/assets/js/Utils.js"></script>
    <script src="~/assets/js/Global.js"></script>
</head>
<body class="container">
    @RenderBody()
    @RenderSection("LibScripts", false)
    @RenderSection("CustScripts", false)
    @StackExchange.Profiling.MiniProfiler.RenderIncludes(showTimeWithChildren: true, position: RenderPosition.Left)
</body>
</html>