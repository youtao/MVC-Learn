<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @RenderSection("Styles", false)
    <link href="~/Content/bootstrap.min.css?v=@FileVersion.CSS" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css?v=@FileVersion.CSS" rel="stylesheet" />
    <link href="~/node_modules/perfect-scrollbar/dist/css/perfect-scrollbar.min.css?v=@FileVersion.CSS" rel="stylesheet" />
    <link href="~/node_modules/toastr/build/toastr.min.css?v=@FileVersion.CSS" rel="stylesheet" />
    <link href="~/assets/css/Admin/_app.layout.min.css?v=@FileVersion.CSS" rel="stylesheet" />
</head>
<body>
    @RenderBody()
    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js?v=@FileVersion.JS"></script>
    <script src="~/node_modules/vue/dist/vue.min.js?v=@FileVersion.JS"></script>
    <script src="~/Scripts/js-cookie/js.cookie-2.1.3.min.js"></script>
    <script src="~/Scripts/jquery.ba-resize.min.js?v=@FileVersion.JS"></script>
    <script src="~/node_modules/perfect-scrollbar/dist/js/perfect-scrollbar.jquery.min.js?v=@FileVersion.JS"></script>
    <script src="~/node_modules/toastr/build/toastr.min.js?v=@FileVersion.JS"></script>
    <script src="~/assets/js/utils.min.js?v=@FileVersion.JS"></script>
    <script src="~/assets/js/global.config.min.js?v=@FileVersion.JS"></script>
    <script src="~/assets/js/_app.layout.min.js?v=@FileVersion.JS"></script>
    @RenderSection("LibScripts", false)
    @RenderSection("CustScripts", false)
</body>
</html>