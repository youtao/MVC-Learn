
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/themes/default/easyui.min.css" rel="stylesheet" />
    <link href="~/assets/css/admin/common.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-2.2.4.min.js"></script>
    <script src="~/Scripts/jquery.easyui-1.4.5.min.js"></script>
    <script src="~/Scripts/locale/easyui-lang-zh_CN.min.js"></script>
    <script src="~/Scripts/js-cookie/js.cookie.min.js"></script>
</head>
<body class="easyui-layout">
    @*<div data-options="region:'north',border:false" style="height:80px;"></div>*@
    <div data-options="region:'west',split:true,title:'导航'" style="width:200px;"></div>
    <div data-options="region:'center'">
        @*<div id="tabs-main">
            </div>*@
        <div id="tt" class="easyui-tabs" data-options="border:false,fit:true">
            <div title="Tab1">
                <iframe src="/admin/menu" style="width:100%;height:99%;" scrolling="auto" frameborder="0" ></iframe>
            </div>
        </div>
    </div>
    <script>
        $('#tabs-main')
            .tabs({
                border: false
            });
    </script>
</body>
</html>
